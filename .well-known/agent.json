{
  "name": "Market Moodboard Agent",
  "description": "Analyzes top 10 cryptocurrency coins by market cap and determines market mood and narrative using Jatevo AI models (zai-glm-4.6). Returns structured mood analysis with emoji indicators, narratives, and confidence scores.",
  "url": "http://localhost:8787/",
  "version": "0.0.1",
  "capabilities": {
    "streaming": false,
    "pushNotifications": false,
    "stateTransitionHistory": true,
    "extensions": [
      {
        "uri": "https://github.com/google-agentic-commerce/ap2/tree/v0.1",
        "description": "Agent Payments Protocol (AP2)",
        "required": true,
        "params": {
          "roles": ["merchant"]
        }
      }
    ]
  },
  "defaultInputModes": ["application/json"],
  "defaultOutputModes": ["application/json", "text/plain"],
  "skills": [
    {
      "id": "analyzeMoodboard",
      "name": "analyzeMoodboard",
      "description": "Fetch top coins from CoinGecko and analyze market mood using Jatevo AI",
      "inputModes": ["application/json"],
      "outputModes": ["application/json", "text/plain"],
      "streaming": false,
      "x_input_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
          "limit": {
            "description": "Number of top coins to analyze (default: 10)",
            "type": "integer",
            "minimum": 1,
            "maximum": 50,
            "default": 10
          },
          "vs_currency": {
            "description": "Currency for price data (default: usd)",
            "type": "string",
            "default": "usd"
          }
        },
        "additionalProperties": false
      },
      "x_output_schema": {
